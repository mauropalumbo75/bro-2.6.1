# Cluster configuration with two logger nodes.

[logger-1]
type=logger
host=localhost

[logger-2]
type=logger
host=localhost

[manager]
type=manager
host=localhost

[proxy-1]
type=proxy
host=localhost

[worker-1]
type=worker
host=localhost
interface=eth0

[worker-2]
type=worker
host=localhost
interface=eth1
