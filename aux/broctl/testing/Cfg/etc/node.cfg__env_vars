# Cluster configuration with a node-specific env_vars specified.

[manager]
type=manager
host=localhost

[proxy-1]
type=proxy
host=localhost

[worker-1]
type=worker
host=localhost
interface=eth0
env_vars=myVar1="some Node val",MYVAR3=nodeval3, Myvar4 = "one;value",myvar6='$PATH'

[worker-2]
type=worker
host=localhost
interface=eth1

